{
    "types": {
        "input": {
            "Int": {
                "kind": "scalar",
                "type": "Int",
                "description": "The `Int` scalar type represents non-fractional signed whole numeric values. Int can represent values between -(2^31) and 2^31 - 1."
            },
            "String": {
                "kind": "scalar",
                "type": "String",
                "description": "The `String` scalar type represents textual data, represented as UTF-8 character sequences. The String type is most often used by GraphQL to represent free-form human-readable text."
            },
            "DateTimeISO": {
                "kind": "scalar",
                "type": "DateTimeISO",
                "description": "A date-time string at UTC, such as 2007-12-03T10:15:30Z, compliant with the `date-time` format outlined in section 5.6 of the RFC 3339 profile of the ISO 8601 standard for representation of dates and times using the Gregorian calendar.This scalar is serialized to a string in ISO 8601 format and parsed from a string in ISO 8601 format."
            },
            "EmailAutomationType": {
                "kind": "enum",
                "type": "EmailAutomationType",
                "description": "Email automation type",
                "values": [
                    "BuiltIn",
                    "Custom"
                ]
            },
            "Float": {
                "kind": "scalar",
                "type": "Float",
                "description": "The `Float` scalar type represents signed double-precision fractional values as specified by [IEEE 754](https://en.wikipedia.org/wiki/IEEE_floating_point)."
            },
            "EmailTemplateStatus": {
                "kind": "enum",
                "type": "EmailTemplateStatus",
                "description": "The status of an email template",
                "values": [
                    "Draft",
                    "Active",
                    "Inactive"
                ]
            },
            "EmailContentFormat": {
                "kind": "enum",
                "type": "EmailContentFormat",
                "description": "The format of an email content",
                "values": [
                    "HTML",
                    "Plain"
                ]
            },
            "PaginationInput": {
                "kind": "object",
                "type": "PaginationInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": true,
                        "name": "itemsPerPage"
                    },
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": false,
                        "name": "itemIndex"
                    }
                ]
            },
            "QueryWaitListEntriesInput": {
                "kind": "object",
                "type": "QueryWaitListEntriesInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": true,
                        "name": "itemsPerPage"
                    },
                    {
                        "kind": "scalar",
                        "type": "Int",
                        "required": false,
                        "name": "itemIndex"
                    },
                    {
                        "kind": "scalar",
                        "type": "DateTimeISO",
                        "required": false,
                        "name": "startDate"
                    },
                    {
                        "kind": "scalar",
                        "type": "DateTimeISO",
                        "required": false,
                        "name": "endDate"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "dateColumn",
                        "validation": [
                            {
                                "type": "isIn",
                                "constraints": [
                                    [
                                        "contactedAt",
                                        "notifiedAt",
                                        "createdAt",
                                        "updatedAt"
                                    ]
                                ]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "waitListIdentifier"
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "waitListId",
                        "validation": [
                            {
                                "type": "isUuid"
                            }
                        ]
                    }
                ]
            },
            "WaitListCreationInput": {
                "kind": "object",
                "type": "WaitListCreationInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "identifier",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [
                                    16
                                ]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": true,
                        "name": "title",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [
                                    256
                                ]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "description",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [
                                    512
                                ]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "emailAutomationKey",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [
                                    64
                                ]
                            }
                        ]
                    }
                ]
            },
            "WaitListUpdateInput": {
                "kind": "object",
                "type": "WaitListUpdateInput",
                "fields": [
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "id",
                        "validation": [
                            {
                                "type": "isUuid"
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "identifier",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [
                                    16
                                ]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "title",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [
                                    256
                                ]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "description",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [
                                    512
                                ]
                            }
                        ]
                    },
                    {
                        "kind": "scalar",
                        "type": "String",
                        "required": false,
                        "name": "emailAutomationKey",
                        "validation": [
                            {
                                "type": "maxLength",
                                "constraints": [
                                    64
                                ]
                            }
                        ]
                    }
                ]
            },
            "Boolean": {
                "kind": "scalar",
                "type": "Boolean",
                "description": "The `Boolean` scalar type represents `true` or `false`."
            },
            "__TypeKind": {
                "kind": "enum",
                "type": "__TypeKind",
                "description": "An enum describing what kind of type a given `__Type` is.",
                "values": [
                    "SCALAR",
                    "OBJECT",
                    "INTERFACE",
                    "UNION",
                    "ENUM",
                    "INPUT_OBJECT",
                    "LIST",
                    "NON_NULL"
                ]
            },
            "__DirectiveLocation": {
                "kind": "enum",
                "type": "__DirectiveLocation",
                "description": "A Directive can be adjacent to many parts of the GraphQL language, a __DirectiveLocation describes one such possible adjacencies.",
                "values": [
                    "QUERY",
                    "MUTATION",
                    "SUBSCRIPTION",
                    "FIELD",
                    "FRAGMENT_DEFINITION",
                    "FRAGMENT_SPREAD",
                    "INLINE_FRAGMENT",
                    "VARIABLE_DEFINITION",
                    "SCHEMA",
                    "SCALAR",
                    "OBJECT",
                    "FIELD_DEFINITION",
                    "ARGUMENT_DEFINITION",
                    "INTERFACE",
                    "UNION",
                    "ENUM",
                    "ENUM_VALUE",
                    "INPUT_OBJECT",
                    "INPUT_FIELD_DEFINITION"
                ]
            }
        }
    }
}